<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask domain='subject' desc='linear discriminant-based decoding for pilab Volume' modality='MRI'>
            
            <qsub>
                <timeBase>0.5</timeBase>
                <memoryBase>0.3</memoryBase>
            </qsub>
            <permanenceofoutput>3</permanenceofoutput>

            <!-- parameters for roidata_lindisc -->
            <split></split>
            <sgolayK></sgolayK>
            <sgolayF></sgolayF>
            <cvsplit></cvsplit>
            <covariatedeg>adaptive</covariatedeg>
            <targetlabels></targetlabels>
            <ignorelabels></ignorelabels>
            <glmclass>CovGLM</glmclass>
            <glmvarargs>
                <item></item>
            </glmvarargs>

            <nperm>1</nperm>
            <nboot>0</nboot>
            <contrastpath></contrastpath>
            
            <inputstreams>
                <stream>pilab_epi</stream>
                <stream>pilab_design</stream>
                <stream>pilab_rois</stream>
            </inputstreams>
            <outputstreams>
                <stream>pilab_decoder_t_sess</stream>
                <stream>pilab_decoder_t_mean</stream>
                <stream>pilab_decoder_nulldist_sess</stream>
                <stream>pilab_decoder_nulldist_mean</stream>
                <stream>pilab_decoder_bootdist_sess</stream>
                <stream>pilab_decoder_bootdist_mean</stream>
            </outputstreams>
            
        </currenttask>
    </tasklist>
</aap>
