            <!-- for here -->
            <maxn>200</maxn>
            <pluginpath></pluginpath>
            <target>mean</target>
            <runstandardplots>1</runstandardplots>

            <inputstreams>
                <stream>pilab_rdms_group</stream>
                <stream>pilab_rdms_rfx</stream>
                <stream>pilab_stimuli</stream>
            </inputstreams>
            <outputstreams>
            </outputstreams>
            
        </currenttask>
